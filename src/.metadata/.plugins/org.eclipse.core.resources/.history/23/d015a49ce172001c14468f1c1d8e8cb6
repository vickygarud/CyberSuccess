package Day_Dec09;

public class AscciValues {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

		int j = 32;
		int n=35;
		while (j < 122) 
		{
			// System.out.println(j);
			char i = 32;
			while (i <= 122)
			{
				
				System.out.print(j + "    =>      " + i + "    | ");
				
				if(j==n)
				{
					System.out.println("\n");
					n=n+4;
					
				}
				i++;

				j++;

			}
		}
	}

}

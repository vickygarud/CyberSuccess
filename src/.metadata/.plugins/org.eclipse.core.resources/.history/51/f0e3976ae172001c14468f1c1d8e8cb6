package Day_Jan10;

class second
{
	int a=100;
	static float b=200.0f;
}

public class StaticEx3 {
	
	void m1(int b)
	{
		System.out.println("Inside m1 b: "+b);
		System.out.println("Inside m1 b: "+second.b);
	}
	public static void main(String[] args) {
		
		second ob = new second();	
		System.out.println("a: "+ob.a);
		System.out.println("b: "+second.b); 
		StaticEx3 se = new StaticEx3();
		se.m1(ob.a);
		
	}

}

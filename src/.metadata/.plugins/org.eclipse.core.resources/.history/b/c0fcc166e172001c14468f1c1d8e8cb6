package Day_Jan02;

import java.util.Scanner;

public class MatrixMultiplication {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("For Matrix A: ");
		System.out.print("Enter no of rows: ");
		int r=sc.nextInt();
		
		System.out.print("Enter no of columns: ");
		int c=sc.nextInt();
		
		int a[][]=new int[r][c];
		
		int row=1;
		for(int i=0; i<a.length; i++)
		{
			int col=1;
			for(int j=0; j<a.length; j++)
			{
				System.out.print("Enter value of row no "+row+" and col no "+col+" : ");
				a[i][j]=sc.nextInt();
				col++;
			}
			
			row++;
		}
		
		
		for(int i=0; i<a.length; i++)
		{
			for(int j=0; j<a.length; j++)
			{
				System.out.print(a[i][j]+" ");
			}
			
			System.out.println();
				
		}
		
		sc.close();
		
		
		/*
		 * System.out.println("Matrix B: "); System.out.println("Enter no of rows"); int
		 * rb=sc.nextInt();
		 * 
		 * System.out.println("Enter no of columns"); int cb=sc.nextInt();
		 * 
		 * int b[][]=new int[rb][cb];
		 */
	}

}
